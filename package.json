{
  "name": "@caplugins/capacitor-meta-sdk",
  "version": "1.0.0",
  "description": "Capacitor plugin for Meta SDK integration",
  "main": "dist/plugin.cjs.js",
  "module": "dist/plugin.js",
  "types": "dist/docs.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "CapacitorMetaSdk.podspec"
  ],
  "author": "Vish <hello@iamvishnu.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caplugins/capacitor-meta-sdk.git"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "meta",
    "facebook",
    "sdk"
  ],
  "scripts": {
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "prepare": "husky",
    "lint": "eslint . --ext ts",
    "fmt": "prettier \"**/*.{css,html,ts,js,java}\" --write"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "type": "module",
  "dependencies": {
    "@capacitor/core": "^8.1.0"
  },
  "devDependencies": {
    "@capacitor/android": "^8.1.0",
    "@capacitor/cli": "^8.1.0",
    "@capacitor/ios": "^8.1.0",
    "@ionic/eslint-config": "^0.4.0",
    "@ionic/prettier-config": "^4.0.0",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-java": "^2.8.1",
    "rimraf": "^6.1.3",
    "rollup": "^4.59.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.9.3"
  }
}